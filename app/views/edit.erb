<form method="POST" action="/posts/<%= @post.id%>">
<input id="hidden" type="hidden" name="_method" value="patch">
<label for="name">Name:</label><input type="text" id="name" name="name" value="<%= @post.name%>">
<label for="content">Post:</label><input type="text" id="content" name="content" value="<%= @post.content%>">

<input type="submit" value="submit" id="submit">

</form>
